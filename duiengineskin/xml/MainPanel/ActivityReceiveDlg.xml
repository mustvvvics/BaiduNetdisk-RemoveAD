<layer name="ActivityReceiveDlg" width="474" height="526" minsize="474,526" appwin="0" ncRect="12,12,12,12"  resize="1" translucent="0" nomaxsize="0">
    <body>
        <dlg pos="0,0,-0,-0" class="cls_dlg_frame">
            <caption name="dlg_activity_caption" pos="0,0,-0,38">
                <img pos="10,8" skin="common_logo_main"></img>
                <text pos="39,9" name="activity_title" maxwidth="420" class="cls_dlg_title_text">%str4130%</text>
                <imgbtn name="btn_close" skin="btn_sys_close" pos="-34,9,@32,@20" cursor="arrow" tip="%str250%" animate="0"/>
            </caption>
            <dlg name="dlg_activity_page_view" pos="1,38,-1,-1">
                <dlg pos="0,0,-0,-0">
                    <webbrowser name="activity_web_page" pos="0,0,-0,-0" use_ie="0" show="1"/>
                    <!--网络连接错误面板-->
                    <dlg name="NetworkErrorPanel" pos="0,0,-0,-0" show="0">
                        <dlg name="netErrTipPanel" pos="|-90,|-100" width="180" height="200">
                            <img pos="|-52,0" skin="web_load_error" width="103" height="93" />
                            <text name="errTip" pos="0,110,-0,130" class="cls_txt_red">%str3453%</text>
                            <imgbtn name="btn_retry" class="defultbtn" pos="|-48,162" width="96" height="34" cursor="arrow">%str3454%</imgbtn>
                        </dlg>
                    </dlg>
                </dlg>
            </dlg>
        </dlg>
    </body>
</layer>
