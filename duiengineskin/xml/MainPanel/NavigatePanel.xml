<dlg name="NavigatePanel" pos="0,0,-0,-0" skin="navigatepanel_bg">
  <dlg pos="0,0,-13,-0">
    <!--首页-->
    <imgbtn name="btn_home" pos="0,0,-0,@80" skin="main_panel_frame" icoxoffset="13" icoyoffset="15" 
    crtext="636D7E" font="0000:微软雅黑" icoskin="navigatepanel_home_btn" 
    txtyoffset="50" cursor="arrow">%str1650%</imgbtn>
    <!--传输-->
    <imgbtn name="btn_trans" pos="0,80,-0,@80" skin="main_panel_frame" icoxoffset="13" icoyoffset="15" 
    crtext="636D7E" font="0000:微软雅黑" icoskin="navigatepanel_transfer_btn" 
    txtyoffset="50" cursor="arrow">%str1651%</imgbtn>
    <gifplayer name="gif_trans" pos="13,95,@42,@42" skin="gif_navigatepanel_transfer_gray" bulltimer="1" show="0" msgtransparent="1"/>
    <img name="img_trans_pause" pos="13,95,@42,@42" skin="navigatepanel_transfer_pause_gray" show="0"/>
    <!-- 数量角标 -->
    <dlg name="dlg_transnum" pos="0,80,-0,@80" msgtransparent="1">
      <img name="img_transnum" pos="40,16,@16,@16" skin="titlepanel_redbg" show="0">
        <text name="txt_transnum" pos="0,|-7,-0,@15" crtext="ffffff" font2="facename:Arial;adding:-1" align="center" valign="middle"></text>
      </img>
    </dlg>

    <!--好友-->
    <imgbtn name="btn_share" pos="0,160,-0,@80" skin="main_panel_frame" icoxoffset="13" icoyoffset="15" 
    crtext="636D7E" font="0000:微软雅黑" icoskin="navigatepanel_share_btn" 
    txtyoffset="50" cursor="arrow">%str1652%</imgbtn>
    <img name="img_share_reddot" pos="50,[-60,@8,@8" skin="titlepanel_reddot" show="0"/>
      
    <dlg name="dlg_invalid_area" pos="0,320,-1,-100">
      <dlg name="dlg_tools" pos="0,-187,-1,@181">
        <hr pos="13,0,-13,0" size="1" crbg="EDEDED"/>
        <!-- 工具 -->
        <listboxex name="listview_tools" pos="0,[1,-0,@180" virtual="1" itemheight="65">
          <template>
            <dlg pos="0,0,-0,-0">
              <imgbtn name="item_img_tool" pos="19,14,@32,@32" skin="function_lock_img" 
              cursor="arrow"/>
              <dlg name="dlg_sync_status" pos="0,0,@25,@16" msgtransparent="1">
                  <img name="img_sync_num" pos="43,8,@20,@16" skin="titlepanel_redbg" show="0">
                      <text name="txt_syncnum" pos="0,|-7,-0,@15" crtext="ffffff" font2="facename:Arial;adding:-1" align="center" valign="middle"></text>
                  </img>
                  <img name="img_sync_status" pos="43,28,@14,@14" skin="titlepanel_redbg" show="0"/>
              </dlg>
              <text name="item_txt_tool" pos="0,52,-0,-0" crtext="636D7E" font="0000:微软雅黑" align="center">%str1653%</text>
            </dlg>
          </template>
        </listboxex>
      </dlg>
    </dlg>

    <!--工具-->
    <imgbtn name="btn_tool" pos="0,-100,-0,@80" skin="main_panel_frame" icoxoffset="13" icoyoffset="15" 
    crtext="636D7E" font="0000:微软雅黑" icoskin="navigatepanel_moretools_btn" 
    txtyoffset="50" cursor="arrow">%str1653%</imgbtn>
    <img name="img_tool_reddot" pos="50,[-60,@8,@8" skin="titlepanel_reddot" show="0"/>

    <!-- 右边线 -->
    <hr pos="-1,0,-1,-0" size="1" mode="vertical" crbg="F0F0F0"/>
  </dlg>
</dlg>